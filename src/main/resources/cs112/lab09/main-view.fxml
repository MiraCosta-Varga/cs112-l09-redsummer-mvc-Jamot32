<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.text.*?>

<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" prefHeight="437.0" prefWidth="319.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs112.lab09.controllers.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label fx:id="welcomeText" alignment="TOP_CENTER" prefHeight="18.0" prefWidth="137.0" text="Interactive History map" textAlignment="CENTER">
        <font>
            <Font name="System Bold" size="12.0" />
        </font></Label>
    <Label alignment="TOP_CENTER" text="Red Summer Edition" textAlignment="CENTER" />
    <Label alignment="TOP_CENTER" contentDisplay="TOP" minHeight="-Infinity" text="DESCRIPTION FOR APP ################################################################" textAlignment="CENTER" wrapText="true" />
    <ImageView fx:id="logoImageView" fitHeight="150.0" fitWidth="200.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true" />
    <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
        <children>
            <Button fx:id="startButton" onAction="#OnStartButtonClicked" text="Start" />
            <Button fx:id="quitButton" mnemonicParsing="false" onAction="#OnQuitButtonClicked" text="Quit" />
        </children>
    </HBox>
</VBox>